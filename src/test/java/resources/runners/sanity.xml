<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Sanity Suite" verbose="1">
    <groups>
        <run>
            <include name="Sanity"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.luma.customlistners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="Sanity Test">
        <classes>
            <class name="com.luma.testsuite.GearTest"></class>
            <class name="com.luma.testsuite.MenTest"></class>
            <class name="com.luma.testsuite.WomensJacketsTest"></class>
        </classes>
    </test>
</suite>